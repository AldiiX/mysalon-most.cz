            <div class="center">
                <h1 class="minitext-colored"><span>────</span>&nbsp;&nbsp;&nbsp;MY SALON&nbsp;&nbsp;&nbsp;<span>────</span></h1>
                <h2 class="h">Rezervace</h2>
            </div>
        </section>



        <section id="page1">
            <div style="justify-content: center; text-align: center">
                <p class="p">Rezervační systém momentálně není dostupný.</p>
                <h2 class="h">Objednávky přijímáme na číslo <span style="color: var(--accent-color)">+420 476 108 686</span>.</h2>
            </div>
        </section>



        @await Html.PartialAsync("Footer")
    </div>
</body>



<script type="module">
     import { vue } from "/scripts/reservations.js";
</script>
@await Html.PartialAsync("PageTitle")
</html>


